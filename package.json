{
  "name": "babel-plugin-jsx-code",
  "version": "1.0.1",
  "description": "Replace `<Code>` content to render code side by side with source string",
  "keywords": [
    "babel-plugin"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/babel-plugin-jsx-code.git",
  "homepage": "https://github.com/christophehurpeau/babel-plugin-jsx-code",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "ts3.1/*"
      ]
    }
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf dist",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "eslint --ext src/",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "test:typings": "yarn link && yarn link babel-plugin-jsx-code && tsc --jsx react --noEmit ts3.1/Component.test.tsx",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.1.5"
  },
  "dependencies": {
    "@babel/generator": "^7.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-react": "7.0.0",
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "@types/jest": "23.3.13",
    "@types/react": "16.7.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-latest-node": "3.0.1",
    "eslint": "5.12.1",
    "eslint-config-pob": "25.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.1",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-junit-reporter": "1.1.0",
    "lint-staged": "8.0.4",
    "pob-babel": "23.22.2",
    "pob-release": "4.6.3",
    "prettier": "1.16.0",
    "react": "16.7.0",
    "typescript": "3.2.4",
    "yarnhook": "0.3.0"
  }
}
